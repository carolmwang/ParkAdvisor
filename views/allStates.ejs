<%- include('./partials/header') %>

<form action="/parks" method="GET">
  <label for="name">
  Choose a state!
  <select type="custom-selects" style="width:200px" name="code">
    <% states.forEach((state) => { %>
      <label for="<%= state.code %>">
      <option id="<%= state.code %>" value="<%= state.code %>" name="<%= state.code %>"><%= state.name %></option>
      </label>
    <% }) %>
  </select>
  <input type="submit" value="submit">
  </label>
</form>

<%- include('./partials/foot') %>